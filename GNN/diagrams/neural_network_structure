digraph {
	A [label="Input Features (x)"]
	B [label="GCNConv (Layer 1)"]
	C [label="BatchNorm (Layer 1)"]
	D [label="ReLU Activation"]
	E [label=Dropout]
	F [label="GCNConv (Layer 2)"]
	G [label="BatchNorm (Layer 2)"]
	H [label="Latent Representation (z)"]
	I [label="Edge Labels (edge_label_index)"]
	J [label="Decoder Output (Decoded Edges)"]
	K [label="Prob Adj Matrix"]
	L [label="Decoded Edges (decode_all)"]
	A -> B
	B -> C
	C -> D
	D -> E
	E -> F
	F -> G
	G -> H
	H -> I [label="Input for Decoder"]
	H -> J [label=Decode]
	H -> K [label="Probabilistic Reconstruction"]
	K -> L [label="Decoded Edges"]
	rankdir=LR
	node [fillcolor=lightblue shape=box style=filled]
}
